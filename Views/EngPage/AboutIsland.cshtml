<div class="card">
<div class="container center-align">
    <br>
   <h5 class="left-align">
       About Island
   </h5>
   <hr>
<div class="row">
    <div class="col s12  left-align">
        
        @foreach (var item in  ViewBag.about)
        {
            <h5 class="dh " >@item.Title</h5>
        <hr>
            <img src="~/LinkPic/@item.ImgUrl" class="responsive-img" width="700px" height="300px">
             <p class="dh" style="direction: rtl;text-align: justify;">
            @item.Text
        </p>
        }
       
    </div>
</div>
</div>
</div>